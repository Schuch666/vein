library(icosa)
g <- icosa::trigrid(tessellation = 6)
plot3d(g, guides = FALSE, sphere = FALSE)
v <- vertices(g, output = "")
f <- faces(g)
## note we need to treat the colours in triplets explicitly to match
## the face index
rgl::rgl.triangles(v[t(f), ], col = rep(c("firebrick", "darkgrey", "dodgerblue"), each = 3))
